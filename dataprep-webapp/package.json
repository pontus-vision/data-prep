  "description": "Talend Data Preparation",
  "main": "src/app/index-module.js",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./coverage",
    "clean:build": "rimraf ./build",
    "clean:coverage": "rimraf ./coverage",
    "build": "cmf-settings && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress",
    "build:dist": "cmf-settings && cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress",
    "prebuild": "rimraf ./build",
    "start": "cmf-settings && cross-env NODE_ENV=development node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack.config.dev.js --progress --content-base build/",
    "test": "cross-env NODE_ENV=test karma start && jest",
    "test:karma": "cross-env NODE_ENV=test karma start",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test karma start karma.conf.ci.js && jest",
    "test:auto": "cross-env NODE_ENV=test karma start --auto",
    "lint:es": "eslint --config .eslintrc --fix src",
    "lint:sass": "sass-lint -c .sass-lint.yml 'src/app/**/*.scss' -v -q",
    "stats": "webpack --config config/webpack.config.dev.js --profile -j > stats.json",
    "extract-i18n": "npm run extract-i18n-components && npm run extract-i18n-containers && npm run extract-i18n-forms && npm run extract-i18n-tdp-app && npm run extract-i18n-tdp-cmf",
    "extract-i18n-components": "i18next node_modules/@talend/react-components/lib   -r --default-values -o src/app/next/locales -n tui-components  -l en,fr,ja --write-old false --fileFilter '*.js' --directoryFilter '!__snapshots__,!__tests__'",
    "extract-i18n-containers": "i18next node_modules/@talend/react-containers/lib   -r --default-values -o src/app/next/locales -n tui-containers  -l en,fr,ja --write-old false --fileFilter '*.js' --directoryFilter '!__snapshots__,!__tests__'",
    "extract-i18n-forms": "i18next node_modules/@talend/react-forms/lib             -r --default-values -o src/app/next/locales -n tui-forms       -l en,fr,ja --write-old false --fileFilter '*.js' --directoryFilter '!__snapshots__,!__tests__'",
    "extract-i18n-tdp-app": "i18next src/app/next                                   -r --default-values -o src/app/next/locales -n tdp-app         -l en,fr,ja --write-old false --fileFilter '*.js' --directoryFilter '!__snapshots__,!__tests__' --keep-removed --ignore-variables",
    "extract-i18n-tdp-cmf": "cmf-settings"
  },
  "jest": {
    "testRegex": "/src/.*\\.test.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/src/test/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
