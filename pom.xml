                                        <include>org/apache/hadoop/conf/Configuration</include>
                                        <include>org/apache/hadoop/conf/Configuratio.class</include>
                                        <include>org/apache/hadoop/conf/**</include>
                                        <include>com/**</include>
                                        <include>uk/**</include>
                                    </includes>
                                </filter>
                                <filter>
                                    <artifact>org.janusgraph:*</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                        <!--<exclude>org/slf4j/**</exclude>-->
                                        <!--<exclude>ch/qos/**</exclude>-->
                                        <!-- LPPM - WARNING: THE OLAP jobs NEED our own custom version of this class: -->
                                        <exclude>org/apache/hadoop/conf/Configuration.class</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration*</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration**</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuratio*</exclude>

                                    </excludes>
                                </filter>
                                <filter>
                                    <artifact>*:*</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                        <!--<exclude>org/slf4j/**</exclude>-->
                                        <!--<exclude>ch/qos/**</exclude>-->


                                    </excludes>
                                </filter>
                                <filter>
                                    <artifact>org.apache.hbase:hbase-shaded-client</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                        <!--<exclude>org/slf4j/**</exclude>-->
                                        <!--<exclude>ch/qos/**</exclude>-->
                                        <!-- LPPM - WARNING: THE OLAP jobs NEED our own custom version of this class: -->
                                        <exclude>org/apache/hadoop/conf/Configuration.class</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration*</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration**</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuratio*</exclude>


                                    </excludes>
                                </filter>
                                <filter>
                                    <artifact>org.apache.hbase:hbase-shaded-client</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                        <!--<exclude>org/slf4j/**</exclude>-->
                                        <!--<exclude>ch/qos/**</exclude>-->
                                        <!-- LPPM - WARNING: THE OLAP jobs NEED our own custom version of this class: -->
                                        <exclude>org/apache/hadoop/conf/Configuration.class</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration*</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration**</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuratio*</exclude>


                                    </excludes>
                                </filter>
                                <filter>
                                    <artifact>org.apache.hbase:hbase-shaded-server</artifact>
                                    <excludes>
                                        <exclude>META-INF/*.SF</exclude>
                                        <exclude>META-INF/*.DSA</exclude>
                                        <exclude>META-INF/*.RSA</exclude>
                                        <!--<exclude>org/slf4j/**</exclude>-->
                                        <!--<exclude>ch/qos/**</exclude>-->
                                        <!-- LPPM - WARNING: THE OLAP jobs NEED our own custom version of this class: -->
                                        <exclude>org/apache/hadoop/conf/Configuration.class</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration*</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuration**</exclude>
                                        <exclude>org/apache/hadoop/conf/Configuratio*</exclude>

                                    </excludes>
                                </filter>
                                <filter>
                                    <artifact>pv-gdpr:pontus-gdpr-graph</artifact>
                                    <includes>
                                        <include>org/**</include>
                                        <include>org/apache/hadoop/conf/Configuratio*</include>
                                        <include>org/apache/hadoop/conf/Configuration**</include>
                                        <include>org/apache/hadoop/conf/Configuration</include>
                                        <include>org/apache/hadoop/conf/Configuratio.class</include>
                                        <include>org/apache/hadoop/conf/**</include>
                                        <include>com/**</include>
                                        <include>uk/**</include>
                                    </includes>
                                </filter>

                            </filters>
                            <transformers>
                                <transformer
                                        implementation="org.apache.maven.plugins.shade.resource.ServicesResourceTransformer"/>
                            </transformers>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.gmavenplus</groupId>
                <artifactId>gmavenplus-plugin</artifactId>
                <version>1.5</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>addSources</goal>
                            <goal>generateStubs</goal>
                            <goal>compile</goal>
                            <goal>removeStubs</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <sources>
                        <source>
                            <directory>${project.basedir}/src/main/java/com/pontusvision/gdpr</directory>
                            <includes>
                                <include>**/*.groovy</include>
                            </includes>
                        </source>
                    </sources>
                </configuration>
            </plugin>


        </plugins>

    </build>
